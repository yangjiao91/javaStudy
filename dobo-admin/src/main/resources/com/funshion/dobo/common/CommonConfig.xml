<?xml version="1.0" encoding="UTF-8"  standalone="no"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<plugins>
		<!-- 用于分页的拦截器 -->
		<plugin interceptor="com.funshion.dobo.base.dao.PaginationInterceptor">
			<!-- 要拦截的方法的这则表达式 -->
			<property name="pageSqlIdRegExp" value="^.*ListAutoPaging$" />
		</plugin>
	</plugins>
</configuration>
