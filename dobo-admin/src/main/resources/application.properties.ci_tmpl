# mysql version database setting
jdbc.driver=com.mysql.jdbc.Driver
jdbc.url=${jdbc.url}
jdbc.username=${jdbc.username}
jdbc.password=${jdbc.password}

# dbcp settings
dbcp.maxIdle=${dbcp.maxIdle}
dbcp.maxActive=${dbcp.maxActive}

# kafka settings
kafka.zk.url=172.16.12.84:2181/confluent
kafka.default.partitions=3
kafka.default.replica.factor=2
kafka.zk.timeout=30000

# schema settings
schema.registry.baseUrl=http://172.16.12.109:8081
schema.registry.initCapacity=256